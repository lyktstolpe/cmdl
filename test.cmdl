signal c, d, e, f

signal a, b <= Xor(c, d) and f

c, d <= 1

f <= 0

signal g <= Xor(c, d)
signal h, i <= Xor(c, d)

signal aa, bb, cc, dd <= Xor.Nor(a, b, c, d, e, f)

component Xor(a, b, c) => x, y
    x, y, u, v, w <= Xor(a, b)

    component Xor(a, b) => x
        x <= a and not b
        signal c
    end
    component Nor(a, b, c, d, e, f) => x, y, z, u, v, w
        x, y, z, u, v, w <= not a
    end
    signal u,v,w
end
